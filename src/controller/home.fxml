                  <Blend />
               </effect>
               <font>
                  <Font name="System Italic" size="18.0" />
               </font>
            </Label>
            <Button fx:id="butSignIn" layoutX="366.0" layoutY="18.0" mnemonicParsing="false" onAction="#showSignin" prefHeight="36.0" prefWidth="88.0" style="-fx-background-color: #00ff00; -fx-border-color: #000000;" text="Sign In" textFill="#171616" />
            <Button fx:id="butsignUp" layoutX="489.0" layoutY="18.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="88.0" style="-fx-background-color: #00ff00; -fx-border-color: #000000;" text="Sign Up" />
