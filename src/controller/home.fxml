            </ImageView>
            <Separator halignment="LEFT" layoutY="71.0" prefWidth="600.0" />
            <ImageView fx:id="slogan" fitHeight="161.0" fitWidth="270.0" layoutX="14.0" layoutY="175.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/title-logo.png" />
               </image>
            </ImageView>
            <Label fx:id="textslogan1" layoutX="14.0" layoutY="108.0" prefHeight="67.0" prefWidth="264.0" text="&quot;The key to success to build your dream&quot;" textAlignment="CENTER" textOverrun="CLIP" wrapText="true">
               <effect>
                  <Blend />
               </effect>
               <font>
                  <Font name="System Italic" size="18.0" />
               </font>
            </Label>
            <ImageView fx:id="logo2" fitHeight="443.0" fitWidth="443.0" layoutX="239.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
